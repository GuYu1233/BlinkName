int led = D7;
void setup() {
    pinMode(led,OUTPUT);

}

void loop() {
    //G
    digitalWrite(led,HIGH);
    delay(3000);
    digitalWrite(led,LOW);
    delay(1000);
    
    digitalWrite(led,HIGH);
    delay(3000);
    digitalWrite(led,LOW);
    delay(1000);
    
    digitalWrite(led,HIGH);
    delay(1000);
    digitalWrite(led,LOW);
    
    delay(3000);
    //U

digitalWrite(led,HIGH);
    delay(1000);
    digitalWrite(led,LOW);
    delay(1000);
    
    digitalWrite(led,HIGH);
    delay(1000);
    digitalWrite(led,LOW);
    delay(1000);
    
    digitalWrite(led,HIGH);
    delay(3000);
    digitalWrite(led,LOW);
    
    delay(7000);
}
